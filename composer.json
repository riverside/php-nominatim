{
  "name": "riverside/php-nominatim",
  "description": "PHP client for Nominatim, a search engine for OpenStreetMap data.",
  "type": "library",
  "version": "1.2.0",
  "keywords": ["php", "osm", "geocoding", "nominatim", "openstreetmap"],
  "homepage": "https://github.com/riverside/php-nominatim",
  "license": "MIT",
  "authors": [
    {
      "name": "Dimitar Ivanov",
      "email": "biggie@abv.bg",
      "homepage": "https://github.com/riverside",
      "role": "Developer"
    }
  ],
  "require": {
    "ext-json": "*",
    "ext-curl": "*",
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "8"
  },
  "autoload": {
    "psr-4": {
      "Nominatim\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nominatim\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
